<p id="notice"><%= notice %></p>

<p>
  <strong>Name:</strong>
  <%= @team.name %>
</p>

<%= link_to 'Edit', edit_team_path(@team) %> |
<%= link_to 'Back', teams_path %>

<%= form_for :assign_project_member, url: assign_project_member_team_path do |f| %>
 

   
<div class="field">
    <%= f.label :member %>
    <select name="member">
        <%@team.users.each do |user|%>
        <option value="<%=user.id%>"><%= user.email%></option>
        <%end%>
    </select>
</div>

<div class="field">
    <%= f.label :project %>
    <select name="project">
        <%@team.projects.each do |project|%>
        <option value="<%=project.id%>"><%= project.name%></option>
        <%end%>
    </select>
</div>
  
  

 <%=f.submit%>
<%end%>