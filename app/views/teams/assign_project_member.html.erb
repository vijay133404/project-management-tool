<%= form_for :assign_project_member, url: submit_project_member_team_path do |f| %>
    <div class="field">
    <%= f.label :member %>
     <%= f.select(:user_id, @team.users.collect {|u| [ u.email, u.id ] }, { include_blank: true },class: "form-control", :required => true)%>
</div>
<div class="field">
    <%= f.label :project %>
    <%= f.select(:project_id, @team.projects.collect {|project| [ project.email, project.id ] }, { include_blank: true },class: "form-control", :required => true)%>
</div>
 
 <%=f.submit%>
<%end%>
<%= link_to 'Back', teams_path %>
